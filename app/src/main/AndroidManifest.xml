<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trashparadise.lifemanager">

    <application
        android:name=".LifeManagerApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LifeManager">
        <activity
            android:name=".ui.bills.BillCheckActivity"
            android:exported="false" />
        <activity
            android:name=".ui.bills.BillAuditActivity"
            android:exported="false" />
        <activity
            android:name=".ui.works.WorkEditActivity"
            android:exported="false" />
        <activity
            android:name=".ui.bills.BillEditActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>